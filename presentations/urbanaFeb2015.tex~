%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\section{Ideal point estimation and bridges}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\frame {                      % SLIDE

    \frametitle{Stochastic spatial voting}

% \begin{center}
%  \begin{tikzpicture}[scale=1,rotate=0]
%   \def\xo{0,0}
%   \def\xu{4,0}
%   \def\xj{2.7,0}
%   \def\m{2,0}
%   \draw (-1,0) -- (5,0);
%   \fill[red] (\xo) circle (1.5pt) node [below=2pt,black] {$nay$};
%   \fill[green] (\xu) circle (1.5pt) node [below=2pt,black] {$aye$};
%   \fill[black] (\xj) circle (1.5pt) node [below=2pt,black] {$x_j$};
%   \draw (2,.1) -- (2,-.1) node [below=2pt,black] {$m$};
% %  \fill[black!50] (\xj) circle (1.5pt) node [below=2pt,black] {$j$};
%  \end{tikzpicture}
% \end{center}

\begin{center}
\begin{tabular}{l}
 \begin{tikzpicture}[scale=.75,rotate=0]
  \def\xnp{0,0}
  \def\xn{-1,0}
  \def\xy{3,2}
  \def\xju{1.25,2}
  \def\xjd{1.25,1}
  \def\xjt{1.25,0}
  \draw[white] (-2,0) -- (4,0);
  \fill[green] (\xn) circle (1.5pt) node [above=2pt,black] {$aye$};
  \fill[red] (\xy) circle (1.5pt) node [above=2pt,black] {$nay$};
  \draw[black!35,dotted] (\xn) -- (\xy);
  \draw (2,-1) -- (0,3) node[very near end,sloped,below] {~~~\small {$y=a+b \cdot x$}}; % cutline
  \fill[black] (\xju) circle (1.5pt) node[above=2pt,black] {$j$};
  \draw (-2,-1) -- (4,-1) node[very near end,sloped,below] {\footnotesize{~~~~~~~~~~~ $x$}};
  \draw (-2,-1) -- (-2,3) node[very near end,sloped,above] {\footnotesize{~~~~~ $y$}};
 \end{tikzpicture}
\end{tabular}
\end{center}


\bigskip

%Vote propensity: $v^*_{j}= \delta(x_j - m) +\text{error}$.
Vote propensity: $v^*_{ij}= \delta_i(a_i + b_i x_j - y_j) +\text{error}_{ij}$.

\bigskip

Voting is sincere: $v_{ij}=
 \begin{cases}
  1 \text{ (`aye')} \iff v^*_{ij} \geq 0 \\
  0 \text{ (`nay') otherwise.}           \,
 \end{cases}$

% \bigskip \pause

% Dynamics: $x_{j,t} \sim \mathrm{N}( x_{j,t-1},\text{slack}
% )$~~~~~(cf.\ Martin\&Quinn 2002).

\bigskip

Estimation: NOMINATE, OC, MCMC (Carroll et al. 2009). 

}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\frame {                      % SLIDE

    \frametitle{Rotation and comparability problems}

 \begin{columns}[c]

 \column{.5\textwidth}

\bi

\item Space lacks ``meaning'' (origin, scale arbitrary)

\item Interpretation: \\ (a) fix known extremists or \\ (b) fix known votes

\item Yet won't allow inter-temporal or inter-chamber comparisons 

\ei

 \column{.5\textwidth}

 \begin{tikzpicture}[scale=1,rotate=0]
  \def\xj{0,0}
  \def\xk{3,0}
  \def\xl{4,0}
  \draw (-.5,0) -- (4.5,0);
  \fill[brown] (\xj) circle (1.5pt) node [below=2pt,black] {$j$};
  \fill[blue] (\xk) circle (1.5pt) node [below=2pt,black] {$k$};
  \fill[green] (\xl) circle (1.5pt) node [below=2pt,black] {$l$};
 \end{tikzpicture}

 \begin{tikzpicture}[scale=1,rotate=0]
  \def\xj{0,0}
  \def\xk{1.5,0}
  \def\xl{2,0}
  \draw (-.5,0) -- (2.5,0);
  \fill[brown] (\xj) circle (1.5pt) node [below=2pt,black] {$j$};
  \fill[blue] (\xk) circle (1.5pt) node [below=2pt,black] {$k$};
  \fill[green] (\xl) circle (1.5pt) node [below=2pt,black] {$l$};
 \end{tikzpicture}

 \begin{tikzpicture}[scale=1,rotate=0]
  \def\xj{0,0}
  \def\xk{-1.5,0}
  \def\xl{-2,0}
  \draw (.5,0) -- (-2.5,0);
  \fill[brown] (\xj) circle (1.5pt) node [below=2pt,black] {$j$};
  \fill[blue] (\xk) circle (1.5pt) node [below=2pt,black] {$k$};
  \fill[green] (\xl) circle (1.5pt) node [below=2pt,black] {$l$};
 \end{tikzpicture}

\pause

 \begin{tikzpicture}[scale=1,rotate=0]
  \def\xj{0,0}
  \def\xk{-1.5,0}
  \def\xm{1,0}
  \draw (1.5,0) -- (-2.5,0);
  \fill[brown] (\xj) circle (1.5pt) node [below=2pt,black] {$j$};
  \fill[blue] (\xk) circle (1.5pt) node [below=2pt,black] {$k$};
  \fill[red] (\xm) circle (1.5pt) node [below=2pt,black] {$m$};
 \end{tikzpicture}

\end{columns}

\bigskip \pause

\textbf{Bridge solution:} \\ (1) members serving both periods/chambers \\
             (2) Assume same ``voting model'' is used \\
(Poole 1998, Groseclose et al 1999)


}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\frame {                      % SLIDE

    \frametitle{Towards a shared space}

\be

\item Roll call votes

\item Survey of members of the lower chamber of Congress

\item Survey of voter attitudes

\ee

\bigskip

\textbf{Bridge material}

(1) and (2) have people in common

(2) and (3) have stimuli in common


\bigskip

(Shor\&McCarty 2011)

}
